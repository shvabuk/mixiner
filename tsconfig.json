{
  "compilerOptions": {
    "experimentalDecorators": true,
    "outDir": "./dist/",
    "allowUnreachableCode": false,
    "allowJs": true,
    "declaration": true,
    "emitDecoratorMetadata": true,
    "lib": ["ES2015"],
    "module": "UMD",
    "moduleResolution": "Node",
    "noImplicitAny": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "removeComments": false,
    "sourceMap": true,
    "target": "ES2015",
    "types": [
       "node"
    ],
    "typeRoots": [
      "./node_modules/@types"
    ]
  },
  "include": [
    "src/mixiner.ts"
  ],
  "compileOnSave": false,
  "buildOnSave": false
}

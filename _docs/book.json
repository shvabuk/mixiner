{
  "gitbook": "3.2.3",
  "title": "Mixiner",
  "author": "Shvab Ostap",
  "plugins": [
    "theme-default",
    "-highlight",
    "-livereload",
    "github",
    "anchorjs",
    "ga",
    "edit-link"
  ],
  "pluginsConfig": {
    "sharing": {
      "facebook": true,
      "twitter": true,
      "google": true,
      "weibo": false,
      "instapaper": false,
      "vk": false,
      "all": ["facebook", "google", "twitter", "weibo", "instapaper", "vk"]
    },
    "edit-link": {
      "base": "https://github.com/shvabuk/mixiner/tree/gh-pages/_docs",
      "label": "Edit This Page"
    },
    "github": {
      "url": "https://github.com/shvabuk/mixiner"
    },
    "ga": {
      "token": "UA-26894117-2"
    }
  }
}

{
  "env": {
    "node": true
  },
  "extends": [
    "tslint:latest",
    "tslint-ionic-rules",
    "tslint-config-airbnb",
    "tslint-config-prettier"
  ],
  "globals": {
      "define": true,
      "module": true
  },
  "rules": {
    "no-increment-decrement": false,
    "max-classes-per-file": false,
    "no-console": false,
    "object-literal-sort-keys": false,
    "no-shadowed-variable": false,
    "no-duplicate-super": false
  }
}
